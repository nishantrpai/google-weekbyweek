{
  "manifest_version": 3,
  "name": "Google Week by Week",
  "version": "0.1.0",
  "description": "Search Google results week by week with a convenient date range picker",
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png",
    "48": "icons/favicon-48x48.png",
    "128": "icons/favicon-128x128.png"
  },
  "action": {
    "default_title": "Google Week by Week"
  },
  "permissions": [],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*",
        "https://google.com/*"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ]
}